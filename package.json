{
  "name": "framework-cli",
  "version": "0.0.1",
  "description": "Gluestack V2 Framework CLI Project",
  "main": "index",
  "scripts": {
    "install:all": "npm install --workspaces --if-present",
    "build:plugins": "npm run build --workspaces --if-present",
    "lint:plugins": "npm run lint --workspaces --if-present",
    "watch:cli": "npm run watch --workspace @gluestack-v2/framework-cli",
    "watch:develop": "npm run watch --workspace @gluestack-v2/glue-plugin-develop",
    "watch:web": "npm run watch --workspace @gluestack-v2/glue-plugin-web",
    "watch:sdk": "npm run watch --workspace @gluestack-v2/glue-plugin-sdk",
    "watch:service-sdk": "npm run watch --workspace @gluestack-v2/glue-plugin-service-sdk",
    "watch:gateway": "npm run watch --workspace @gluestack-v2/glue-plugin-service-gateway",
    "watch:functions": "npm run watch --workspace @gluestack-v2/glue-plugin-functions",
    "watch:router-nginx": "npm run watch --workspace @gluestack-v2/glue-plugin-router-nginx",
    "watch:components": "npm run watch --workspace @gluestack-v2/glue-plugin-components",
    "watch:crons": "npm run watch --workspace @gluestack-v2/glue-plugin-crons",
    "watch:openai": "npm run watch --workspace @gluestack-v2/glue-plugin-openai",
    "watch:events": "npm run watch --workspace @gluestack-v2/glue-plugin-events",
    "watch:middlewares": "npm run watch --workspace @gluestack-v2/glue-plugin-middlewares",
    "watch:database": "npm run watch --workspace @gluestack-v2/glue-plugin-database",
    "watch:graphql": "npm run watch --workspace @gluestack-v2/glue-plugin-graphql",
    "watch:db-client": "npm run watch --workspace @gluestack-v2/glue-plugin-database-client",
    "watch:queues": "npm run watch --workspace @gluestack-v2/glue-plugin-queues",
    "watch:storage": "npm run watch --workspace @gluestack-v2/glue-plugin-storage",
    "watch:storage-client": "npm run watch --workspace @gluestack-v2/glue-plugin-storage-client",
    "watch:deploy": "npm run watch --workspace @gluestack-v2/glue-plugin-deploy",
    "watch:all": "npm run watch:cli & npm run watch:database & npm run watch:graphql & npm run watch:db-client & npm run watch:develop & npm run watch:gateway & npm run watch:crons & npm run watch:sdk & npm run watch:functions & npm run watch:components & npm run watch:web & npm run watch:events & npm run watch:queues & npm run watch:middlewares & npm run watch:queues & npm run watch:deploy & npm run watch:storage & npm run watch:storage-client & npm run watch:sdk",
    "prepare": "husky install",
    "lint": "eslint packages/**/**/src --ext .ts,.tsx --config .eslintrc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gluestack-v2/glue-cli"
  },
  "keywords": [
    "gluestack",
    "v2",
    "fullstack",
    "framework",
    "cli",
    "mobile",
    "web",
    "storybook",
    "shared-components"
  ],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/config-conventional": "^17.3.0",
    "@gluestack-seal/cli": "^0.0.16",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/node": "^18.15.5",
    "commitlint": "^17.3.0",
    "eslint": "^8.29.0",
    "eslint-plugin-ft-flow": "^2.0.3",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.7.1",
    "source-map": "^0.7.4"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@gluestack-v2/glue-plugin-develop": "^0.1.0",
    "@gluestack-v2/glue-plugin-sdk": "^0.0.2",
    "@gluestack-v2/glue-plugin-web": "^0.1.1"
  }
}
