    "develop",
    "cli",
    "mobile",
    "web",
    "storybook",
    "shared-components"
  ],
  "license": "MIT",
  "dependencies": {
    "@gluestack-v2/framework-cli": "^0.1.x",
    "@gluestack-v2/glue-plugin-develop": "^0.1.x",
    "@types/express": "^4.17.17",
    "concurrently": "^8.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "immer": "^10.0.2",
