{
  "name": "todo-app",
  "version": "0.1.0",
  "description": "Gluestack V2 App",
  "main": "index",
  "scripts": {
    "install:all": "npm install --workspaces --if-present",
    "add:functions": "node glue add @gluestack-v2/glue-plugin-develop develop && node glue add @gluestack-v2/glue-plugin-web web && node glue add @gluestack-v2/glue-plugin-sdk sdk && node glue add @gluestack-v2/glue-plugin-service-gateway gateway  && node glue add @gluestack-v2/glue-plugin-functions functions"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gluestack-v2/glue-cli"
  },
  "keywords": [
    "gluestack",
    "v2",
    "fullstack",
    "framework",
    "cli",
    "mobile",
    "web",
    "storybook",
    "shared-components"
  ],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/config-conventional": "^17.3.0",
    "@gluestack-seal/cli": "^0.0.16",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/node": "^18.15.5",
    "commitlint": "^17.3.0",
    "eslint": "^8.29.0",
    "eslint-plugin-ft-flow": "^2.0.3",
    "prettier": "^2.7.1",
    "source-map": "^0.7.4"
  },
  "workspaces": [
    "../../packages/*",
    "client/web",
    "server/functions",
    ".glue/__generated__/packages/**"
  ],
  "dependencies": {
    "@gluestack-v2/glue-plugin-database": "^0.1.1",
    "@gluestack-v2/glue-plugin-database-client": "^0.1.0",
    "@gluestack-v2/glue-plugin-develop": "^0.1.0",
    "@gluestack-v2/glue-plugin-functions": "^0.1.0",
    "@gluestack-v2/glue-plugin-sdk": "^0.1.0",
    "@gluestack-v2/glue-plugin-service-gateway": "^0.1.0",
    "@gluestack-v2/glue-plugin-web": "^0.1.1",
    "@gluestack/ui-next-adapter": "^2.1.7"
  }
}
